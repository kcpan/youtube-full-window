{
    "manifest_version": 2,

    "name": "Youtube Full Window",
    "description": "Makes current Youtube page fill the window.",
    "version": "1.0",
    "page_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "tabs", "declarativeContent"
    ],
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    }
}
